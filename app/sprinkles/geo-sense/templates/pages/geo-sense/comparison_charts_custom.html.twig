        process: false,
        data: $('#chart_2_2').highcharts().getCSV() // when process is false we just push the output from Highcharts
    });

    csv_data.push({
        heading: $('#chart_2_3_daterange_selected_large').html().replace(/,/g , ' -'),
        process: false,

    csv_data.push({
        heading: $('#chart_1_2_daterange_selected_large').html().replace(/,/g , ' -'),
        process: false,
        data: $('#chart_1_2').highcharts().getCSV() // when process is false we just push the output from Highcharts
    });

    csv_data.push({

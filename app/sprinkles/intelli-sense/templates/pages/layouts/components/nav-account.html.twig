                            <li>
                                <a href="{{site.uri.public}}/elephantwifi/dashboard">
                                    <i class="fa fa-sign-out fa-fw"></i> Main Portal </a>
                            </li>
                            <li>
                                <a href="{{site.uri.public}}/elephantwifi/wifi_user/dashboard">
                                    <i class="fa fa-user fa-fw"></i> Personal Portal </a>
                            </li>
                        </ul>
                    </li>

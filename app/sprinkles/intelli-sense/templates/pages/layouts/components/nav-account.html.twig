                            {% for venue in current_user.getVenues() %}
                                <li class="venue_id" id="venue_{{venue.id}}">
                                    <a href="#">{{venue.name}}
                                {% if current_user.primary_venue_id == venue.id %}
                                    <i class="fa fa-check fa-fw"></i>
                                {% endif %}
                                </a>
                                </li>
                            {% endfor %}
                        </ul>
                    </li>
                    {% endif %}
